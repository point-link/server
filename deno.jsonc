{
  "tasks": {
    "run":        "deno run   --lock=deno.lock --allow-net --allow-env --allow-read ./src/main.ts",
    "run:watch":  "deno run   --lock=deno.lock --allow-net --allow-env --allow-read --watch ./src/main.ts",
    "cache":      "deno cache --lock=deno.lock ./src/main.ts",
    "cache:lock": "deno cache --lock=deno.lock --lock-write ./src/main.ts"
  }
}
